<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_website_delivery" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.base.setup,website.delivery</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="90"/>
            <field name="inherit_id" ref="website.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='website_info_settings']" position="inside">
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_left_pane"/>
                            <div class="o_setting_right_pane">
                                <span class="o_form_label">Enable Delivery Slot Feature</span>
                                <div class="text-muted content-group mt16">
                                    <field name="enable_delivery_date"/>
                                </div>
                            </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>